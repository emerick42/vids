{% extends 'AppMainBundle::layout.html.twig' %}

{% block content %}
    {% if videos|length > 0 %}
        <ul>
            {% for video in videos %}
                <li><a href="{{ video.url }}">{{ video.url }}</a></li>
            {% endfor %}
        </ul>
    {% else %}
        {{ 'app_main.top.no_video'|trans }}
    {% endif %}
{% endblock %}
